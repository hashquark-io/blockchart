{
  "canisters": {
    "connectd": {
      "main": "src/connectd/main.mo"
    },
    "chart":{
      "main": "src/linkedup/chart.mo",
      "dependencies": [
        "connectd"
    ]
    },
    "token":{
      "main": "src/linkedup/token.mo"
    },
    "linkedup": {
      "main": "src/linkedup/main.mo"
    },
    "linkedup_assets": {
      "dependencies": ["linkedup","chart"],
      "frontend": {
        "entrypoint": "src/linkedup/public/main.js"
      },
      "type": "assets",
      "source": [
        "dist/linkedup_assets",
        "src/linkedup/assets"
      ]
    }
  },
  "defaults": {
    "build": {
      "packtool": ""
    }
  },
  "networks": {
    "ic": {
      "providers": [
        "https://gw.dfinity.network"
      ],
      "type": "persistent"
    },
    "local": {
      "bind": "0.0.0.0:8000",
      "type": "ephemeral"
    }
  },
  "dfx": "0.6.26"
}
